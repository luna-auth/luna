{
  "extends": "astro/tsconfigs/strictest",
  "include": [".astro/types.d.ts", "**/*", "src/env.d.ts"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["node"],
    "paths": {
      "@/*": ["src/*"]
    },
    "exactOptionalPropertyTypes": false,
    "moduleResolution": "bundler",
    "strict": true
  }
}
